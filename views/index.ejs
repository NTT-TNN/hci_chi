<%- include head.ejs %>
  <div style="text-align:center;">
    <a href="https://github.com/NTT-TNN"><i style="font-size:48px;color:'red'" class="fa fa-github" ></i>
    </a>

  </div>
  <h1 style="text-align:center">Đề Cương HCI</h1>
   <% for(var i=0; i<obj.features.length&&i<2232; i++) {%>
     <% if (obj.features[i].properties.Field18){%>
       <div><%= i+1 %>.<%= obj.features[i].properties.Field18 %></div>
       <div style="padding-left:40px">
         <div class="row">
           <div class="col-md-6">A. <%= obj.features[i].properties.Field19 %> </div>
           <div class="col-md-6" >B.<%= obj.features[i].properties.Field20 %></div>
        </div>
        <div class="row">
         <div class="col-md-6">C. <%= obj.features[i].properties.Field21 %></div>
         <div class="col-md-6"> D. <%= obj.features[i].properties.Field22 %></div>
       </div>
         <div>E. <%= obj.features[i].properties.Field23 %></div>
       </div>
     <div style="padding-left:20px">
         <div>=> Đáp án: <%= obj.features[i].properties.Field24 %></div>
         <div>Giải thích: <%= obj.features[i].properties.Field25 %></div>
     </div>
     <%}else{%>
       <div><%= i+1 %>.<%= obj.features[i].properties.Field18 %> Không có câu hỏi</div>
     <%}%>

   <%}%>
   <div style="text-align:'center'" class="row">
     <a href="https://github.com/NTT-TNN"><i style="font-size:48px;color:'red'" class="fa fa-github" ></i>
     </a>
     <div class="col-xs-12 col-md-12 col-sm-12">
       <img src="/images/IMG_4778.JPG" style="height:768px;width:1366px"alt="">

     </div>
   </div>


</body>
</html>
